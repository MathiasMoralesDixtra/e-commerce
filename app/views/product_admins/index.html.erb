<h1>Product Admins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_admins.each do |product_admin| %>
      <tr>
        <td><%= link_to 'Show', product_admin %></td>
        <td><%= link_to 'Edit', edit_product_admin_path(product_admin) %></td>
        <td><%= link_to 'Destroy', product_admin_path(product_admin), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Admin', new_product_admin_path %>
